{
  "name": "e-commerce-projects",
  "version": "1.0.0",
  "description": "E-commerce with MERN STACK",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node backend/index.js",
    "server": "nodemon backend/index.js",
    "client": "npm start --prefix frontend",
